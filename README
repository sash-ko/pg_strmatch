make
sudo make install
sudo su postgres -c 'psql -d DBNAME -f /usr/share/postgresql/PGVERSION/contrib/pg_strmatch_install.sql'
sudo su postgres -c 'psql -d DBNAME -f test.sql'
sudo su postgres -c 'psql -d DBNAME -f uninstall.sql'

